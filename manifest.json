{
	"name": "LeadInfo",
	"version": "0.1",
	"description": "Get Lead Details from Linkedin and add to SalesFlare",
	"permissions": ["activeTab"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["jquery-3.5.1.slim.min.js", "popper.min.js", "bootstrap.min.js", "page.js"]
		}
	],
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/leadinfo16.png",
			"32": "images/leadinfo32.png",
			"48": "images/leadinfo48.png",
			"128": "images/leadinfo128.png"
		}
	},
	"icons": {
		"16": "images/leadinfo16.png",
		"32": "images/leadinfo32.png",
		"48": "images/leadinfo48.png",
		"128": "images/leadinfo128.png"
	},
	"manifest_version": 2,
	"author": "Faizan Ayubi"
}